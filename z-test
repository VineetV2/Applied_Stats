#question1
library(BSDA)
data <- c(88, 92, 94, 94, 96, 97, 97, 97, 99, 99, 105, 109,
          109, 109, 109, 110, 112, 112, 113, 114, 115)

result <- z.test(data, mu = 100, sigma.x = 15, sigma.y = NULL)

print(result)

#Question2
cityA <- c(82,84,85,89,91,91,92,94,99,99,105,109,110,112,112,113,114)
cityB <- c(90,91,91,91,95,95,99,99,108,109,109,114,115,116,117,128,129,130,133)
a=z.test(x=cityA, y =cityB, mu = 0, sigma.x= 15, sigma.y=15)
print(a)
